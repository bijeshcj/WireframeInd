<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/emergency_screened_background"
                tools:context="com.verizontelematics.indrivemobile.activity.EmergencyServicesActivity">

    <include
        android:id="@+id/headerLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        layout="@layout/actionbar_header"/>

    <TextView
        android:padding="10dp"
        android:layout_below="@+id/headerLayout"
        android:text="@string/emergency_services_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</RelativeLayout>
