package com.verizontelematics.indrivemobile.models.data;

import java.util.HashMap;

/**
 * Created by z688522 on 9/11/14.
 */
public class BaseData {

    // Used to store key and value pair.
    // for nested structures use the BaseData again as value.
    private HashMap<String, Object>mMap = new HashMap<String, Object>();

    public Object get(String key) { return mMap.get(key); }

    public void set(String key, Object obj) {
        mMap.put(key, obj);
    }
}
